{
  "name": "choose-station",
  "homepage": "https://aberratio.github.io/choose-station/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@rjsf/core": "^5.4.0",
    "@rjsf/mui": "^5.4.0",
    "@rjsf/utils": "^5.4.0",
    "@rjsf/validator-ajv8": "^5.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.9"
  },
  "scripts": {
    "before-first-run": "npm install",
    "start": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "run-cypress": "npx cypress run --browser chrome",
    "open-cypress": "npx cypress open --browser chrome"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^12.9.0",
    "gh-pages": "^5.0.0"
  }
}
